<RocketeProject>
	<FilePaths>
		<!-- all these nodes can be duplicated to allow for several sources. bear in mind the source search is recursive -->
		<!-- relative paths are also supported-->
		<Font>font</Font>
		<Texture>textures</Texture>
		<Interface>interface</Interface>
	</FilePaths>
	<Preferences>
		<!-- project specific preferences: default window size, background color/pic, ... -->
		<FilePaths>
			<WordList>word_lists</WordList> <!-- do not duplicate -->
			<LocalizationOpeningTag>${</LocalizationOpeningTag> <!-- do not duplicate -->
			<LocalizationClosingTag>}</LocalizationClosingTag> <!-- do not duplicate -->
			<SnippetsFolder>snippets</SnippetsFolder><!-- do not duplicate; can have subfolder full of snippets... -->
		</FilePaths>
	</Preferences>
</RocketeProject>