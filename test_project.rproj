<RocketeProject>
	<FilePaths>
		<!-- all these nodes can be duplicated to allow for several sources. bear in mind the source search is recursive -->
		<!-- relative paths are also supported-->
		<Font>font</Font>
		<Texture>textures</Texture>
		<Interface>interface</Interface>
		<Font>P:/creatures/RESOURCE/FONTS</Font>
		<Texture>P:/creatures/RESOURCE/TEXTURES</Texture>
		<Interface>P:/creatures/RESOURCE/INTERFACE/ROCKET</Interface>
		<LocalizationFile>P:/creatures/RESOURCE/LOCALIZATION/creatures4_project.xml</LocalizationFile> <!-- do not duplicate; takes a pina locada project, other files MUST be in same folder -->
	</FilePaths>
	<Preferences>
		<!-- project specific preferences: default window size, background color/pic, ... -->
		<FilePaths>
			<WordList>C:/Projets/Rockete/word_lists</WordList> <!-- do not duplicate -->
			<LocalizationOpeningTag>${</LocalizationOpeningTag> <!-- do not duplicate -->
			<LocalizationClosingTag>}</LocalizationClosingTag> <!-- do not duplicate -->
			<SnippetsFolder>C:/Projets/Rockete/snippets</SnippetsFolder><!-- do not duplicate; can have subfolder full of snippets... -->
		</FilePaths>
	</Preferences>
</RocketeProject>