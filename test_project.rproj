<RocketeProject>
	<FilePaths>
		<!-- all these nodes can be duplicated to allow for several sources. bear in mind the source search is recursive -->
		<!-- relative paths are also supported-->
		<Font>font</Font>
		<Texture>textures</Texture>
		<Interface>interface</Interface>
		<Font>P:/creatures/RESOURCE/FONTS</Font>
		<Texture>P:/creatures/RESOURCE/TEXTURES</Texture>
		<Interface>P:/creatures/RESOURCE/INTERFACE/ROCKET</Interface>
	</FilePaths>
	<Preferences>
		<!-- project specific preferences: default window size, background color/pic, ... -->
		<FilePaths>
			<WordList>C:/Projets/Rockete/word_lists</WordList> <!-- do not duplicate -->
			<LocalizationFile>P:/creatures/RESOURCE/LOCALIZATION/creatures4_project.xml</LocalizationFile> <!-- takes a pina locada project, other files MUST be in same folder -->
			<LocalizationOpeningTag>${</LocalizationOpeningTag>
			<LocalizationClosingTag>}</LocalizationClosingTag>
		</FilePaths>
	</Preferences>
</RocketeProject>